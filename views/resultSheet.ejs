<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Result Sheet</title>
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
      integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z"
      crossorigin="anonymous"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Assistant&display=swap"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="/app.css" />
    <script src="https://kit.fontawesome.com/b155107641.js"></script>
  </head>
  <body>
    <div
      class="container-fluid d-flex justify-content-center align-items-center"
      id="head-section"
    >
      <h5 class="font-weight-bolder">Result Data</h5>
    </div>
    <div class="container mt-5 result">
      <div class="row">
        <div
          class="p-1 col-xs-3 result-col d-flex justify-content-center align-items-center"
        >
          <span class="font-weight-bolder">Order Num</span>
        </div>
        <div
          class="p-1 col-xs-3 result-col d-flex justify-content-center align-items-center"
        >
          <span class="font-weight-bolder">Profit/Loss (%)</span>
        </div>
        <div
          class="p-1 col-xs-3 result-col d-flex justify-content-center align-items-center"
        >
          <span class="font-weight-bolder">Transferred Amount</span>
        </div>
        <div
          class="p-1 col-xs-3 result-col d-flex justify-content-center align-items-center"
        >
          <span class="font-weight-bolder">Total Marketplace Charges</span>
        </div>
      </div>
      <% data.forEach(result=>{ %>
      <div class="row">
        <div
          class="p-1 col-xs-3 result-col d-flex justify-content-center align-items-center"
        >
          <%= result.orderNum %>
        </div>
        <div
          class="p-1 col-xs-3 result-col d-flex justify-content-center align-items-center"
        >
          <%= result.proLoss %>
        </div>
        <div
          class="p-1 col-xs-3 result-col d-flex justify-content-center align-items-center"
        >
          <%= result.transferredAmount %>
        </div>
        <div
          class="p-1 col-xs-3 result-col d-flex justify-content-center align-items-center"
        >
          <%= result.marketPlace%>
        </div>
      </div>
      <% }) %>
      <div class="row mb-3">
        <div
          class="col-xs-12 d-flex justify-content-center align-items-center btn-row"
        >
          <a href="/" class="btn btn-md submit-btn">Go to home page</a>
        </div>
      </div>
    </div>
  </body>
</html>
